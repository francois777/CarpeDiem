.row
  .col-xs-12.col-md-12
    %section.tariffs.clearfix
      %h1
        = t(".all_tariffs")
      .table-responsive
        %table.table.tariffs
          %thead
            %tr
              %td
                %b
                  = t(".tariff_category")
              %td
                %b
                  = t(".tariff")
              %td
                %b
                  = t(".effective_date")
              %td
                %b
                  = t(".end_date")
          %tbody
            = render @tariffs || t(".no_tariffs_found")
      